//@version=5
strategy("Example Converted Strategy", overlay=true, default_qty_type=strategy.percent_of_equity, default_qty_value=10)

// This is an example of a converted MQL4/5 strategy to Pine Script
// Original MQL4 strategy would have been converted to Pine Script format

// Input parameters
fastLength = input.int(12, "Fast Length")
slowLength = input.int(26, "Slow Length")

// Converted logic from MQL4
fastMa = ta.ema(close, fastLength)
slowMa = ta.ema(close, slowLength)

// Generate signals (converted from MQL4 logic)
longCondition = ta.crossover(fastMa, slowMa)
shortCondition = ta.crossunder(fastMa, slowMa)

// Execute trades (converted from MQL4 logic)
if (longCondition)
    strategy.entry("Long", strategy.long)
    
if (shortCondition)
    strategy.entry("Short", strategy.short)

// Plotting
plot(fastMa, "Fast MA", color=color.blue)
plot(slowMa, "Slow MA", color=color.red)

// Plot shapes for entry signals
plotshape(longCondition, "Long Entry", shape.triangleup, location.belowbar, color.green, size=size.small)
plotshape(shortCondition, "Short Entry", shape.triangledown, location.abovebar, color.red, size=size.small)